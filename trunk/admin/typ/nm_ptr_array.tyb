member procedure debug is
l_ptr_array ptr_array := self;
begin
  for i in 1..l_ptr_array.pa.last loop
    nm_debug.debug( 'Ptr '||to_char(l_ptr_array.pa(i).ptr_id)||','||to_char(l_ptr_array.pa(i).ptr_value));
  end loop;
end;

member function get_id ( p_value in integer ) return integer is
l_ptr_array ptr_array := self;
retval integer := -1;
begin
  for i in 1..l_ptr_array.pa.last loop
    if l_ptr_array.pa(i).ptr_value = p_value then
   retval := l_ptr_array.pa(i).ptr_id;
 end if;
  end loop;
  return retval;
end;

end;
